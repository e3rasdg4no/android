<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:layout_width="match_parent"
    android:layout_height="260dp"
    android:layout_centerVertical="true"
    android:padding="3dp"
    android:layout_marginRight="5dp"
    android:layout_marginLeft="5dp"
    xmlns:android="http://schemas.android.com/apk/res/android">
        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="match_parent"
            android:layout_height="40dp">
            <TextView
                android:id="@+id/titleDialog"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/hijau"
                android:text="Tomat, 20Kg - Rp. 20.000"
                android:layout_centerVertical="true"
                android:paddingRight="60dp"/>
            <View
                android:layout_width="2dp"
                android:layout_height="match_parent"
                android:background="@color/hijau"
                android:id="@+id/view8"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/textClose" />
            <ImageView
                android:id="@+id/close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/closebutton"
                android:layout_alignParentRight="true"/>
            <TextView
                android:id="@+id/textClose"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:textColor="@color/hijau"
                android:text="Tutup"
                android:paddingLeft="10dp"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/close"/>
        </RelativeLayout>
        <View
            android:id="@+id/view9"
            android:layout_width="match_parent"
            android:layout_height="2.5dp"
            android:background="@color/hijau"
            android:layout_below="@+id/relativeLayout2"
            android:layout_marginTop="10dp"/>
        <RelativeLayout
            android:id="@+id/relativeLayout28"
            android:layout_width="match_parent"
            android:layout_height="110dp"
            android:background="@color/hijau"
            android:layout_below="@+id/view9"
            android:layout_marginTop="10dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingTop="20dp"
            android:paddingBottom="20dp">
            <LinearLayout
                android:id="@+id/linnn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Jumlah : "
                    android:textColor="#ffffff"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <EditText
                        android:id="@+id/jumlahInDialogHistori"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff"
                        android:textColorHighlight="@color/transparent"
                        android:background="@color/transparent"/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#ffffff"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/lannn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/linnn"
                android:layout_marginTop="25dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Harga   : "
                    android:textColor="#ffffff"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <EditText
                        android:id="@+id/hargaInDialogHistori"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:textColor="#ffffff"
                        android:background="@color/transparent"
                        android:textColorHint="@color/abutulisan"
                        android:textColorHighlight="@color/transparent"
                        android:hint="Rp. "/>
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="#ffffff"/>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/relativeLayout28"
            android:layout_marginTop="15dp">
            <View
                android:id="@+id/view10"
                android:layout_width="15dp"
                android:layout_height="1dp"
                android:layout_centerHorizontal="true"/>
            <Button
                android:id="@+id/saveInDialogHistori"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/view10"
                android:background="@drawable/bg_rounded2"
                android:text="Masukkan"
                android:textColor="#ffffff"
                android:textAllCaps="false"/>
            <Button
                android:id="@+id/batalInDialogHistori"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/view10"
                android:background="@drawable/bg_rounded3"
                android:text="Batal"
                android:textColor="@color/hijau"
                android:textAllCaps="false"/>
        </RelativeLayout>
    </RelativeLayout>