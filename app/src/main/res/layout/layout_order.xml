<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e9e9e9">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="20dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="NOMOR ORDER"
            android:id="@+id/textView146"
            android:layout_alignTop="@+id/space2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:gravity="center"
            android:layout_toStartOf="@+id/textView159"
            android:layout_toLeftOf="@+id/textView159" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="TOTAL HARGA"
            android:id="@+id/textView147"
            android:layout_alignTop="@+id/space2"
            android:layout_alignRight="@+id/txttotalharga"
            android:layout_alignEnd="@+id/txttotalharga"
            android:gravity="center"
            android:layout_alignBottom="@+id/textView159"
            android:layout_toRightOf="@+id/textView159"
            android:layout_toEndOf="@+id/textView159" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Nomor #1"
            android:id="@+id/txtnomororder"
            android:layout_below="@+id/textView146"
            android:layout_marginTop="20dp"
            android:textColor="#000000"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:gravity="center"
            android:layout_toStartOf="@+id/txttanggalorder"
            android:layout_toLeftOf="@+id/txttanggalorder" />

        <Space
            android:layout_width="20px"
            android:layout_height="150dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="30dp"
            android:id="@+id/space2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="IDR 1500000"
            android:id="@+id/txttotalharga"
            android:textColor="#000000"
            android:layout_alignTop="@+id/txtnomororder"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:gravity="center"
            android:layout_alignBottom="@+id/txtnomororder"
            android:layout_toRightOf="@+id/txttanggalorder"
            android:layout_toEndOf="@+id/txttanggalorder" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtnomororder"
            android:layout_alignRight="@+id/txttotalharga"
            android:layout_alignEnd="@+id/txttotalharga"
            android:layout_marginTop="20dp"
            android:id="@+id/relativeLayout18">
            <View
                android:layout_width="fill_parent"
                android:layout_height="1dp"
                android:background="@color/abutulisan"
                android:id="@+id/view"
                android:layout_below="@+id/pendaftaran"
                android:layout_alignLeft="@+id/view3"
                android:layout_alignStart="@+id/view3"
                android:layout_marginTop="10dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/imageView14"
                android:src="@drawable/bulatbiru"
                android:layout_marginLeft="@dimen/left_image_order"
                android:layout_alignTop="@+id/space3"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <Space
                android:layout_width="20px"
                android:layout_height="20px"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/space3" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/imagestatus"
                android:src="@drawable/bulatmerah"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_marginRight="50dp" />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:id="@+id/img_dikirim"
                android:src="@drawable/bulatbiru"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true" />

        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Packing"
            android:id="@+id/textView150"
            android:layout_below="@+id/relativeLayout18"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/txttanggalorder"
            android:gravity="center"
            android:layout_marginTop="20dp"
            android:layout_toLeftOf="@+id/txttanggalorder"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Belum Terkirim"
            android:id="@+id/txtstatus"
            android:gravity="center"
            android:layout_alignTop="@+id/textView150"
            android:layout_alignRight="@+id/txttotalharga"
            android:layout_alignEnd="@+id/txttotalharga"
            android:layout_alignLeft="@+id/txttotalharga"
            android:layout_alignStart="@+id/txttotalharga"
            android:layout_marginBottom="30dp"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="TANGGAL ORDER"
            android:id="@+id/textView159"
            android:gravity="center"
            android:layout_above="@+id/txtnomororder"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="21 Desember 2016"
            android:id="@+id/txttanggalorder"
            android:textColor="#000000"
            android:gravity="center"
            android:layout_alignTop="@+id/txttotalharga"
            android:layout_centerHorizontal="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="Sedang Dikirim"
            android:id="@+id/textView161"
            android:gravity="center"
            android:layout_alignTop="@+id/textView150"
            android:layout_alignRight="@+id/txttanggalorder"
            android:layout_alignEnd="@+id/txttanggalorder"
            android:layout_toRightOf="@+id/textView150"
            android:layout_toEndOf="@+id/textView150"
            android:textSize="12sp" />

    </RelativeLayout>
</RelativeLayout>